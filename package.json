{
  "name": "interviewer.dc",
  "version": "1.0.0",
  "description": "Interviewing platform",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server/server.js",
    "test": "nyc mocha test/ServerSpec.js --reporter mochawesome && casperjs test/EndSpec.js",
    "test-server": "nyc mocha test/ServerSpec.js --reporter mochawesome",
    "db:create": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:unseed": "sequelize db:seed:undo:all",
    "db:reseed": "npm run db:unseed && npm run db:seed",
    "db:setup": "npm run db:create && npm run db:unseed && npm run db:seed",
    "db:drop": "sequelize db:migrate:undo:all",
    "build": "node_modules/.bin/webpack --config webpack.config.js",
    "quick": "npm run build && npm start",
    "start:socket": "nodemon server/socket.io/socket-server.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrr22-lyra-dot-json/interviewer.dc"
  },
  "author": "hrr22-lyra-dot-json",
  "license": "ISC",
  "dependencies": {
    "auth0-lock": "^10.13.0",
    "axios": "^0.16.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "body-parser": "^1.17.1",
    "css-loader": "^0.26.1",
    "express": "^4.15.2",
    "google-auth-library": "^0.10.0",
    "googleapis": "^18.0.0",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
    "path": "^0.12.7",
    "randomstring": "^1.1.5",
    "react-big-calendar": "^0.11.1",
    "react-dom": "^15.3.1",
    "react-modal": "^1.7.3",
    "react-router": "^2.8.0",
    "react-select": "^1.0.0-rc.3",
    "sequelize": "^3.30.4",
    "socket.io": "^1.7.3",
    "style-loader": "^0.16.1",
    "webpack": "^2.2.1"
  },
  "devDependencies": {
    "auth0-lock": "^10.13.0",
    "babel-cli": "^6.6.5",
    "babel-watch": "^2.0.6",
    "casperjs": "*",
    "chai": "*",
    "css-loader": "^0.26.1",
    "mocha": "*",
    "mochawesome": "^2.0.4",
    "moment": "^2.18.1",
    "nodemon": "^1.11.0",
    "nyc": "^10.2.0",
    "pg": "^6.1.5",
    "phantomjs-prebuilt": "*",
    "prop-types": "^15.5.6",
    "react-big-calendar": "^0.11.1",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.0",
    "sequelize-cli": "^2.7.0",
    "socket.io": "^1.7.3",
    "style-loader": "^0.16.1",
    "supertest": "*",
    "webpack-dev-server": "^2.4.2"
  }
}
