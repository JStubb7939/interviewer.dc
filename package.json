{
  "name": "interviewer.dc",
  "version": "1.0.0",
  "description": "Interviewing platform",
  "main": "server.js",
  "scripts": {
    "test": "nyc mocha --timeout 20000 && casperjs test-end/EndSpec.js",
    "start": "node server/server.js",
    "db:create": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:unseed": "sequelize db:seed:undo:all",
    "db:reseed": "npm run db:unseed && npm run db:seed",
    "db:setup": "npm run db:create && npm run db:unseed && npm run db:seed",
    "db:drop": "sequelize db:migrate:undo:all",
    "build": "node_modules/.bin/webpack --config webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrr22-lyra-dot-json/interviewer.dc"
  },
  "author": "hrr22-lyra-dot-json",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "sequelize": "^3.30.4",
    "auth0-lock": "^10.13.0",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.0",
    "webpack": "^2.2.1",
    "path": "^0.12.7",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0"
  },
  "devDependencies": {
    "casperjs": "*",
    "chai": "*",
    "mocha": "*",
    "nyc": "^10.2.0",
    "phantomjs": "*",
    "phantomjs-prebuilt": "*",
    "sequelize-cli": "^2.7.0",
    "supertest": "*",
    "webpack-dev-server": "^2.4.2",
    "babel-cli": "^6.6.5",
    "babel-watch": "^2.0.6",
    "auth0-lock": "^10.13.0",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.0"
  }
}
