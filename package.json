{
  "name": "interviewer.dc",
  "version": "1.0.0",
  "description": "Interviewing platform",
  "main": "server.js",
  "scripts": {
    "test": "nyc mocha --timeout 20000 && casperjs test-end/EndSpec.js",
    "start": "node server/server.js",
    "db:create": "sequelize db:migrate",
    "db:seed": "sequelize db:seed:all",
    "db:unseed": "sequelize db:seed:undo:all",
    "db:reseed": "npm run db:unseed && npm run db:seed",
    "db:setup": "npm run db:create && npm run db:unseed && npm run db:seed",
    "db:drop": "sequelize db:migrate:undo:all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/hrr22-lyra-dot-json/interviewer.dc"
  },
  "author": "hrr22-lyra-dot-json",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.17.1",
    "express": "^4.15.2",
    "morgan": "^1.8.1",
    "sequelize": "^3.30.4",
    "auth0-lock": "^10.13.0",
    "react-dom": "^15.3.1",
    "react-router": "^2.8.0"
  },
  "devDependencies": {
    "casperjs": "*",
    "chai": "*",
    "mocha": "*",
    "nyc": "^10.2.0",
    "phantomjs": "*",
    "phantomjs-prebuilt": "*",
    "sequelize-cli": "^2.7.0",
    "supertest": "*"
      }
}
